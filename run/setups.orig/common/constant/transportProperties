FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


h       100;         //heat transfer coefficient (W/(m^2 K))
rp      0.005;       //particle radius (m)

// Fluid

rhof    1.2;        //fluid density (kg/m^3)
cpf     700;        //fluid specific heat capacity (J/(kg*K)
kf      0.06;       //fluid thermal conductivity (W/(m*K)

mu      17.15e-06;
Tmu     0;
mu2     43.32e-06;
Tmu2    800;

//Source for mu temperature dependency:
//https://www.engineersedge.com/physics/viscosity_of_air_dynamic_and_kinematic_14483.htm

// Solid

rhos    5150;       //stone density (kg/m^3)
cps     1130;       //stone specific heat capacity (J/(kg*K)
ks      0;        //stone phase effective thermal conductivity (W/(m*K)