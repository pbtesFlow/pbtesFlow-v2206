// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


h       200;    //heat transfer coefficient (W/(m^2 K))
rp      0.005;  //particle radius (m)
//Cf      50;   //rhof*cpf*dT/dt = Cf*(Tf-Ts) => dT ~ 1000*dt*Cp*(Tf-Ts) 

// Fluid

rhof    1.2;    //fluid density (kg/m^3)
cpf     700;    //fluid specific heat capacity (J/(kg*K)
kf      0;      //fluid thermal conductivity (W/(m*K)

mu      1.825e-05;
Tmu     293;
mu2     1.963e-5;
Tmu2    343;

//Source for mu temperature dependency:
//https://www.engineersedge.com/physics/viscosity_of_air_dynamic_and_kinematic_14483.htm

// Solid

rhos    3000;   //stone density (kg/m^3)
cps     0.1;    //stone specific heat capacity (J/(kg*K)
ks      10;     //stone phase effective thermal conductivity (W/(m*K)