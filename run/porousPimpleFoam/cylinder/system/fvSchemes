FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
    grad(U)         Gauss linear;
    grad(alpha1)    Gauss linear;
}

divSchemes
{
    div(((rhof*phi)|interpolate(porosity)),U) Gauss linearUpwind grad(Tf);
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;
    div(((rhof*cpf)*phi),Tf) Gauss linearUpwind grad(Tf);
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p_rgh;
    pcorr;
}

// ************************************************************************* //
