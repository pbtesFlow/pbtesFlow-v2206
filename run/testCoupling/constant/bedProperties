// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

rho_b   5150; //density (kg/m^3)
cp_b    1130; //specific heat capacity (J/(kg*K))
r_b     0.005; //radius of sphere (m)
geo_b   1; //geometric factor describing non-sphereness
h       200; //convective heat transfer coefficient (W/(m^2*K))

